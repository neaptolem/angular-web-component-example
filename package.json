{
  "name": "angular-elements-example",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod && npm run build:js && npm run build:css",
    "build:js": "cat dist/angular-elements-example/runtime-es5.*.js dist/angular-elements-example/polyfills-es5.*.js dist/angular-elements-example/custom-elements-es5-adapter.*.js dist/angular-elements-example/scripts.*.js dist/angular-elements-example/main-es5.*.js > example/elem-example.js",
    "build:css": "cat dist/angular-elements-example/styles.*.css > example/elem-example.css",
    "serve": "http-server --gzip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/elements": "^9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "core-js": "^3.6.5",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "document-register-element": "^1.14.3",
    "elements-zone-strategy": "^9.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "http-server": "^0.12.3",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  }
}